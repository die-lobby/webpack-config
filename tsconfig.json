{
  "compilerOptions": {
    "sourceMap": true,
    "module": "esnext",
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.promise",
      "es2015.iterable"
    ],
    "allowJs": false,
    "noImplicitAny": false,
    "removeComments": false,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "moduleResolution": "node",
    "experimentalDecorators": true
  },
  "include": [
    "../../../**/*.ts",
    "../../../**/*.vue"
  ]
}
